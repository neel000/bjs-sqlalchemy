# Include the templates directory and all its contents
include bjs_sqlalchemy/models/*
include bjs_sqlalchemy/serializer/*
include bjs_sqlalchemy/pagination/*
include bjs_sqlalchemy/filters/*

# If you want to include subdirectories and their contents, use the recursive syntax:
recursive-include bjs_sqlalchemy/models *
recursive-include bjs_sqlalchemy/serializer *
recursive-include bjs_sqlalchemy/pagination *
recursive-include bjs_sqlalchemy/filters *
